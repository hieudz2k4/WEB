     userName: '',\n                password: ''\n            }\n        }, \n        computed: {\n            ...mapState({\n                showLogIn: state => state.showLogIn\n            })\n        },\n        props: {\n            \n        },\n        methods: {\n            clickClose() {\n                store.commit('changeShowLogIn')\n            },\n            signIn() {\n                this.countSignIn++;\n                if(this.checkEmpty()) {\n                    console.log(\"empty\")\n                } else {\n                    axios.post('http://localhost:8088/v1/login', {\n                        userName: this.userName,\n                        password: this.password\n                    }).then((res) => {\n                        console.log(res)\n                    })\n                }\n                return true;\n            }, \n            checkEmptyUserName() {\n                return !this.userName.length\n            },\n            checkEmptyPassword() {\n                return !this.password.length \n            },\n            checkEmpty() {\n                return (this.checkEmptyUserName() || this.checkEmptyPassword()) \n            }\n        }\n    }\n\n</script>"],"mappings":"AAoCI,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,KAAI,MAAO,aAAY;AAC9B,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGL,QAAQ,CAAC;MACRM,SAAS,EAAEC,KAAI,IAAKA,KAAK,CAACD;IAC9B,CAAC;EACL,CAAC;EACDE,KAAK,EAAE,CAEP,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACTX,KAAK,CAACY,MAAM,CAAC,iBAAiB;IAClC,CAAC;IACDC,MAAMA,CAAA,EAAG;MACL,IAAI,CAACV,WAAW,EAAE;MAClB,IAAG,IAAI,CAACW,UAAU,CAAC,CAAC,EAAE;QAClBC,OAAO,CAACC,GAAG,CAAC,OAAO;MACvB,OAAO;QACHjB,KAAK,CAACkB,IAAI,CAAC,gCAAgC,EAAE;UACzCb,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC,CAACa,IAAI,CAAEC,GAAG,IAAK;UACbJ,OAAO,CAACC,GAAG,CAACG,GAAG;QACnB,CAAC;MACL;MACA,OAAO,IAAI;IACf,CAAC;IAC