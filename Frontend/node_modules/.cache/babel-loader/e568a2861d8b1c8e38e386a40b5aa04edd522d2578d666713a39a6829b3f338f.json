{"ast":null,"code":"import axios from 'axios';\nimport store from '../store.js';\nimport { mapState } from 'vuex';\nexport default {\n  data() {\n    return {\n      userName: this.$refs.userName.value,\n      password: this.$refs.password.value\n    };\n  },\n  computed: {\n    ...mapState({\n      showLogIn: state => state.showLogIn\n    })\n  },\n  props: {},\n  methods: {\n    clickClose() {\n      store.commit('changeShowLogIn');\n    },\n    signIn() {\n      if (this.checkEmpty()) {\n        console.log(\"empty\");\n      } else {\n        axios.post('http://localhost:8088/v1/login', {\n          userName: this.userName,\n          password: this.password\n        }).then(res => {\n          console.log(res);\n        });\n      }\n    },\n    checkEmpty() {\n      if (length(this.userName) * length(this.password) == 0) {\n        return true;\n      }\n      return false;\n    }\n  }\n};","map":{"version":3,"names":["axios","store","mapState","data","userName","$refs","value","password","computed","showLogIn","state","props","methods","clickClose","commit","signIn","checkEmpty","console","log","post","then","res","length"],"sources":["/Users/macbook/Desktop/Tài Liệu/Bài Tập Lớn /Software Engineering/frontend/src/components/LogInComp.vue"],"sourcesContent":["<template>\n    <div v-if=\"showLogIn\" class=\"backdrop-blur-3xl bg-[#1971CA]/15 flex justify-center rounded-3xl\">\n        <div class=\"text-white p-6\">\n            <h1 class=\"text-3xl font-bold\">Login</h1>\n            <div class=\"mt-[0.75rem]\">\n                <h1 class=\"text-sm\">Email</h1>\n                <input v-model=\"userName\" class=\"text-black outline-none w-[21rem] p-1 pl-1.5 rounded-md\" type=\"email\" placeholder=\"username\">\n            </div>\n            <div class=\"mt-[0.75rem]\">\n                <h1 class=\"text-sm\">Password</h1>\n                <input v-model=\"password\" class=\"text-black outline-none w-[21rem] p-1 pl-1.5 rounded-md\" type=\"password\" placeholder=\"Password\">\n            </div>\n            <h1 class=\"text-sm mt-3 hover:cursor-pointer hover:underline\">Forget Password?</h1>\n            <button class=\"mt-7 bg-[#023464] w-[21rem] p-1 rounded-md\" @click=\"signIn()\">Sign In</button>\n            <h1 class=\"text-center mt-6 text-sm\">Or continue with</h1>\n            <div class=\"flex flex-ro justify-around mt-6\">\n                <img src=\"../assets/google.png\" class=\"size-7 hover:size-10 hover:cursor-pointer\">\n                <img src=\"../assets/github.png\" class=\"size-7 hover:size-10 hover:cursor-pointer\">\n                <img src=\"../assets/facebook.png\" class=\"size-7 hover:size-10 hover:cursor-pointer\">\n            </div>\n            <div class=\"mt-5 flex flex-row text-sm justify-center\">\n                <h1>Don't have an account yet?</h1>\n                <h1 class=\"font-bold hover:cursor-pointer hover:underline\">Register for free</h1>\n            </div>\n        </div>\n        <img @click=\"clickClose()\" class=\"size-6 hover:cursor-pointer mt-6 mr-5\" src=\"../assets/circle.png\">\n    </div>\n</template>\n<script>\n    import axios from 'axios'\n    import store from '../store.js'\n    import { mapState } from 'vuex'\n    export default {\n        data() {\n            return {\n                userName: this.$refs.userName.value,\n                password: this.$refs.password.value\n            }\n        }, \n        computed: {\n            ...mapState({\n                showLogIn: state => state.showLogIn\n            })\n        },\n        props: {\n            \n        },\n        methods: {\n            clickClose() {\n                store.commit('changeShowLogIn')\n            },\n            signIn() {\n                if(this.checkEmpty()) {\n                    console.log(\"empty\")\n                } else {\n                    axios.post('http://localhost:8088/v1/login', {\n                        userName: this.userName,\n                        password: this.password\n                    }).then((res) => {\n                        console.log(res)\n                    })\n                }\n            }, \n            checkEmpty() {\n                if (length(this.userName) * length(this.password) == 0) {\n                    return true;\n                }\n                return false;\n            }\n        }\n    }\n\n</script>"],"mappings":"AA6BI,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,KAAI,MAAO,aAAY;AAC9B,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,IAAI,CAACC,KAAK,CAACD,QAAQ,CAACE,KAAK;MACnCC,QAAQ,EAAE,IAAI,CAACF,KAAK,CAACE,QAAQ,CAACD;IAClC;EACJ,CAAC;EACDE,QAAQ,EAAE;IACN,GAAGN,QAAQ,CAAC;MACRO,SAAS,EAAEC,KAAI,IAAKA,KAAK,CAACD;IAC9B,CAAC;EACL,CAAC;EACDE,KAAK,EAAE,CAEP,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACTZ,KAAK,CAACa,MAAM,CAAC,iBAAiB;IAClC,CAAC;IACDC,MAAMA,CAAA,EAAG;MACL,IAAG,IAAI,CAACC,UAAU,CAAC,CAAC,EAAE;QAClBC,OAAO,CAACC,GAAG,CAAC,OAAO;MACvB,OAAO;QACHlB,KAAK,CAACmB,IAAI,CAAC,gCAAgC,EAAE;UACzCf,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBG,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC,CAACa,IAAI,CAAEC,GAAG,IAAK;UACbJ,OAAO,CAACC,GAAG,CAACG,GAAG;QACnB,CAAC;MACL;IACJ,CAAC;IACDL,UAAUA,CAAA,EAAG;MACT,IAAIM,MAAM,CAAC,IAAI,CAAClB,QAAQ,IAAIkB,MAAM,CAAC,IAAI,CAACf,QAAQ,KAAK,CAAC,EAAE;QACpD,OAAO,IAAI;MACf;MACA,OAAO,KAAK;IAChB;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}